<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="         http://java.sun.com/xml/ns/persistence         http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="ccr2_PU">
    <jta-data-source>jdbc/arquillian</jta-data-source>
    <class>model.Accessibility</class>
    <class>model.Account</class>
    <class>model.Administrator</class>
    <class>model.Car</class>
    <class>model.CarImage</class>
    <class>model.Driver</class>
    <class>model.Employee</class>
    <class>model.Form</class>
    <class>model.Location</class>
    <class>model.Manager</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.ddl-generation" value="drop-and-create-tables"/>
      <property name="eclipselink.logging.level" value="FINE"/>
    </properties>
  </persistence-unit>
</persistence>
